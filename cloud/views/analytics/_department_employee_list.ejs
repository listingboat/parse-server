<p class="table-title-wrap">
    <span>Employee-Level</span>
    Training Performance
</p>
<div class="team-details-list">
    <div class="team-detail-row">
        <span class="team-name-section custom-team-name-section">Employee</span>
        <span>PQ Score</span>
        <span>Best 7 days PQ Gain</span>
        <span>Last 7 days PQ Gain</span>
        <span>Best Training Streak (days)</span>
        <span class="percent-unit-section">Days Since Last Trained</span>
    </div>
    <% for(var userIndex in userList){ %>
        <div class="team-detail-row">
            <span class="team-name-section custom-team-name-section"><%= (userList[userIndex].get('first_name') || '') %> <%= (userList[userIndex].get('last_name') || '') %></span>
            <span><%= typeof userList[userIndex].get('pq_score') === "number" ? userList[userIndex].get('pq_score') : '---' %></span>
            <span><%= typeof userList[userIndex].get('best_7_day_pq_gain') === "number" ? userList[userIndex].get('best_7_day_pq_gain') : '---' %> </span>
            <span><%= typeof userList[userIndex].get('last_7_day_pq_gain') === "number" ? userList[userIndex].get('last_7_day_pq_gain') : '---' %></span>
            <span><%= typeof userList[userIndex].get('longest_streak') === "number" ? userList[userIndex].get('longest_streak') : '---' %></span>
            <span class="percent-unit-section"><%= (userList[userIndex].get('current_streak'))?((userList[userIndex].get('current_streak') <= 0)? Math.abs(userList[userIndex].get('current_streak')) : 0) : '---' %></span>
        </div>
    <% } %>
</div>
<div class="table-footer-wrap clearfix">
    <div class="pagination-wrap js_employee_pagination"
    data-current_page="<%= employeePaginationData.currentPage %>"
    data-user_count="<%= employeePaginationData.count %>"
    data-per_page="<%= employeePaginationData.perPage %>">
    <span> <%= employeePaginationData.displayText %> </span><a href="#" class="pagination-nav-btn ws-icon-left js_employee_prev_page <%= employeePaginationData.isFirstPage? 'disabled' : '' %>"></a><a href="#" class="pagination-nav-btn ws-icon-right js_employee_next_page <%= employeePaginationData.isLastPage? 'disabled' : '' %>"></a>
    </div>
</div>
