<%- contentFor('title') %>
| PDF Report : Doer PQ Details

<%- contentFor('body') %>
<div class="pdf-wrapper clearfix">
	<div class="pq-details-report doer-details-report">
		<div class="report-pq-icon"></div>
		<div class="report-user-details">
			<h4 class="report-user-name">Jason Wesbecher, <span>CMO</span></h4>
			<img class="report-superpower-tag" src="<%= asset('assets/images/doer_superpower_dark.png') %>">
		</div>
		<div class="report-pq-details">
			<h4 class="report-pq-details-head">Doers are adaptable, persuasive and charming.</h4>
			<p class="report-pq-details-text">When communicating and emailing with an doer workstyle, start with the bottom line, keep points short, focus on the actionable, and give things an edge to keep the conversation interesting.</p>
			<a class="web-link-wrap" href="http://my.work.style">my.work.style</a>
		</div>
		<div class="report-logo-wrap">
			<img src="<%= asset('assets/images/workstyle_logo_dark.png') %>">
		</div>
	</div>
	<div class="pq-sidebar-wrap">
		<h4 class="all-pq-list-head">My Workstyle Hierarchy</h4>
		<ul class="all-pq-details all-pq-report">
			<li class="each-pq-detail doer-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-6x"></span></div>
				<h4 class="report-pq-label">Doer</h4>
			</li>
			<li class="each-pq-detail connector-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-5x"></span></div>
				<h4 class="report-pq-label">Connector</h4>
			</li>
			<li class="each-pq-detail organizer-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-4x"></span></div>
				<h4 class="report-pq-label">Organizer</h4>
			</li>
			<li class="each-pq-detail dreamer-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-3x"></span></div>
				<h4 class="report-pq-label">Dreamer</h4>
			</li>
			<li class="each-pq-detail advisor-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-2x"></span></div>
				<h4 class="report-pq-label">Advisor</h4>
			</li>
			<li class="each-pq-detail original-pq-wrap">
				<div class="pq-icon-wrap"><span class="pq-icon-size pq-wrap-1x"></span></div>
				<h4 class="report-pq-label">Original</h4>
			</li>
		</ul>
	</div>
</div>
