<%- contentFor('script') %>
<script type="text/javascript">
    $(document).ready(function () {
    	landing_page_init();
    });
</script>

<%- contentFor('title') %>
| Register

<%- contentFor('body') %>
<%- include _header %>
<div class="content flat-bg">
	<div class="container">
		<div class="reset-form-wrap">
			<a href="/" class="logo-colored">
				<img src="<%= asset('assets/images/login_page_logo.png') %>">
			</a>
			<div class="reset-form-box">
				<h2>Set Your New Workstyle Password</h2>
				<form class="reset-form">
					<div class="form-group js-input-group">
						<input type="password" class="form-control input-common" placeholder="New Password">
						<div class="custom-tooltip">
							<div class="custom-tooltip-arrow"></div>
							<p class="error-text-wrap">* Required Field</p>
						</div>
					</div>
					<div class="form-group js-input-group">
						<input type="password" class="form-control input-common" placeholder="Repeat New Password">
						<div class="custom-tooltip">
							<div class="custom-tooltip-arrow"></div>
							<p class="error-text-wrap">* Required Field</p>
						</div>
					</div>
					<button class="btn green-btn">Confirm</button>
				</form>
			</div>
		</div>
	</div>
</div>
<%- include _footer %>
