<div class="invite-tab-wrap clearfix hide" id="select-question-type-div"
     data-hash_time_stamp="<%= questionTypeHashTimeStamp %>"
     data-setup_url="<%= url('company.initialDepartmentSetup') %>"
     data-default_department_name="<%= defaultDepartment %>"
        >
    <div class="add-department-wrap">
        <h3 class="invite-head">Select which trainings apply to each department</h3>
        <form id="department-question-type-form">
            <div class="select-training-wrap">
                <div class="list-wrapper">
                    <div class="select-wrap">
                        <span class="label-wrap"></span>
                        <span><%= basicQuestionType.name %></span>
                        <% for(var index in questionTypes){ %>
                        <span><%= questionTypes[index].name %></span>
                        <% } %>
                    </div>
                    <div class="select-wrap js_relation_div_sample hide">
                        <span class="label-wrap js_department_label_span"></span>
                        <span><label class="checkbox-select-wrap"><input type="checkbox" checked= "checked" disabled><span class="ws-icon-checkmark" ></span></label></span>
                        <% for(var index in questionTypes){ %>
                        <span><label class="checkbox-select-wrap"><input type="checkbox" class="js_question_types"
                                                                         checked="checked"
                                                                         data-question_type_id="<%= questionTypes[index].id %>"
                                                                         data-question_type_hash="<%= questionTypes[index].hash %>",
                                                                         checked = "checked"
                                        >
                                <span class="ws-icon-checkmark" ></span>
                            </label>
                    </span>
                        <% } %>
                    </div>
                    <div class="btm-btn-wrap">
                        <a class="btn yellow-btn yellow-white-btn js_start_department_setup js_to_department_setup_step_2">BACK</a>
                        <button class="btn yellow-btn" id="setup-department-button">FINISH</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>
