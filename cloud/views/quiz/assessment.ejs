                <div  class="column"></div>
                <div  class="column"><span class="js-answer1"></span></div>
                <div  class="column"><span class="js-answer2"></span></div>
                <div  class="column"><span class="js-answer3"></span></div>
                <div  class="column"><span class="js-answer4"></span></div>
                <div  class="column"><span class="js-answer5"></span></div>
                <div  class="column"><span class="js-answer6"></span></div>
            </div>
        </div>
    </div>
    <div class="row btn-block">
        <button class="btn btn-primary js-previous">&laquo; Previous Question</button>
        <button class="btn btn-primary js-clear">Clear This Question</button>
        <button class="btn btn-primary js-next">Next Question &raquo;</button>
        <button class="btn btn-primary js-submit" style="display: none;">Submit</button>
        <a class="btn btn-primary" href="/quiz/intro?start=0">Help</a>
    </div>
</div>
<div class="loader-container js-loader">
    <div class="loader-bg"></div>
    <img class="loader-img" src="<%= asset('assets/images/loader-big.gif') %>"/>
</div>
